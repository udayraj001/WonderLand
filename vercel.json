{
    "version": 2,
    "builds": [
      {
        "src": "Client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://127.0.0.1:8000//api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/Client/$1"
      }
    ]
  }
  